          <TableColumn fx:id="colId" prefWidth="124.0" text="ID" />
          <TableColumn fx:id="colJumlah" prefWidth="71.0" text="Jumlah" />
            <TableColumn fx:id="colStok" prefWidth="86.0" text="Stok" />
            <TableColumn fx:id="colProduk" prefWidth="19.0" text="Produk" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
